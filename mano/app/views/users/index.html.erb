<div id="login-content">
	<h3>Log In</h3>
	<div id="login-form">
		<%= form_tag log_in_path do %>
		  <p>
	    <%= label_tag :email %><br />
	    <%= text_field_tag :email %>
		  </p>
		  <p>
	    <%= label_tag :password %><br />
	    <%= password_field_tag :password_field %>
		  </p>
		  <p class="button"><%= submit_tag "Log in" %></p>
		<% end %>	
	</div>
	<div id="send-to-registration">
	<%= link_to "Register", new_user_path, id: "registration-link" %>
	</div>

</div>



